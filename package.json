{
  "name": "swap-app-react",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.0.0",
    "@tanstack/react-query": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "viem": "^2.0.0",
    "wagmi": "^2.0.0",
    "web3": "^4.0.0",
    "@solana/web3.js": "^1.91.0",
    "tronweb": "^5.3.1"
